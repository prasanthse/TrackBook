<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/red_bg"
    tools:context=".QrScanActivity">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="153dp"
        android:layout_height="156dp"
        android:layout_marginStart="50dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="50dp"
        android:layout_marginRight="50dp"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo" />

    <Button
        android:id="@+id/homeFromQrBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="28dp"
        android:background="@color/colorPrimaryDark"
        android:text="Home"
        android:textColor="@color/colorAccent"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TableLayout
        android:id="@+id/scanContents"
        android:layout_width="368dp"
        android:layout_height="287dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        android:stretchColumns="2"
        app:layout_constraintBottom_toTopOf="@+id/homeFromQrBtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center">

        <TextView
            android:id="@+id/bookedName"
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:text="Name"
            android:textColor="@color/colorAccent"
            android:textSize="24sp"
            android:textStyle="bold"
            android:layout_weight="1"
            tools:layout_editor_absoluteX="56dp"
            tools:layout_editor_absoluteY="232dp" />

            <TextView
                android:id="@+id/bookedNameResult"
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:text=""
                android:textColor="@color/colorPrimary"
                android:textSize="18dp"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="56dp"
                tools:layout_editor_absoluteY="232dp" />

        </TableRow>


        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

        <TextView
            android:id="@+id/bookedContact"
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:text="Phone Number"
            android:textColor="@color/colorAccent"
            android:textSize="24sp"
            android:textStyle="bold"
            android:layout_weight="1"
            tools:layout_editor_absoluteX="56dp"
            tools:layout_editor_absoluteY="232dp" />

            <TextView
                android:id="@+id/bookedContactResult"
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:text=""
                android:textColor="@color/colorPrimary"
                android:textSize="18dp"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="56dp"
                tools:layout_editor_absoluteY="232dp" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

        <TextView
            android:id="@+id/bookedStart"
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:text="From"
            android:textColor="@color/colorAccent"
            android:textSize="24sp"
            android:textStyle="bold"
            android:layout_weight="1"
            tools:layout_editor_absoluteX="56dp"
            tools:layout_editor_absoluteY="232dp" />

            <TextView
                android:id="@+id/bookedStartResult"
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:text=""
                android:textColor="@color/colorPrimary"
                android:textSize="18dp"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="56dp"
                tools:layout_editor_absoluteY="232dp" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

        <TextView
            android:id="@+id/bookedEnd"
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:text="To"
            android:textColor="@color/colorAccent"
            android:textSize="24sp"
            android:textStyle="bold"
            android:layout_weight="1"
            tools:layout_editor_absoluteX="56dp"
            tools:layout_editor_absoluteY="232dp" />

            <TextView
                android:id="@+id/bookedEndResult"
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:text=""
                android:textColor="@color/colorPrimary"
                android:textSize="18dp"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="56dp"
                tools:layout_editor_absoluteY="232dp" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

        <TextView
            android:id="@+id/bookedTime"
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:text="Time"
            android:textColor="@color/colorAccent"
            android:textSize="24sp"
            android:textStyle="bold"
            android:layout_weight="1"
            tools:layout_editor_absoluteX="56dp"
            tools:layout_editor_absoluteY="232dp" />

            <TextView
                android:id="@+id/bookedTimeResult"
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:text=""
                android:textColor="@color/colorPrimary"
                android:textSize="18dp"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="56dp"
                tools:layout_editor_absoluteY="232dp" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

        <TextView
            android:id="@+id/bookedDate"
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:text="Date"
            android:textColor="@color/colorAccent"
            android:textSize="24sp"
            android:textStyle="bold"
            android:layout_weight="1"
            tools:layout_editor_absoluteX="56dp"
            tools:layout_editor_absoluteY="232dp" />

            <TextView
                android:id="@+id/bookedDateResult"
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:text=""
                android:textColor="@color/colorPrimary"
                android:textSize="18dp"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="56dp"
                tools:layout_editor_absoluteY="232dp" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

        <TextView
            android:id="@+id/bookedClass"
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:text="Class"
            android:textColor="@color/colorAccent"
            android:textSize="24sp"
            android:textStyle="bold"
            android:layout_weight="1"
            tools:layout_editor_absoluteX="56dp"
            tools:layout_editor_absoluteY="232dp" />

            <TextView
                android:id="@+id/bookedClassResult"
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:text=""
                android:textColor="@color/colorPrimary"
                android:textSize="18dp"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="56dp"
                tools:layout_editor_absoluteY="232dp" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        <TextView
            android:id="@+id/bookedSeats"
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:text=""
            android:textColor="@color/colorAccent"
            android:textSize="24sp"
            android:textStyle="bold"
            android:layout_weight="1"
            tools:layout_editor_absoluteX="56dp"
            tools:layout_editor_absoluteY="232dp" />

            <TextView
                android:id="@+id/bookedSeatsResult"
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:text=""
                android:textColor="@color/colorPrimary"
                android:textSize="18dp"
                android:textStyle="bold"
                tools:layout_editor_absoluteX="56dp"
                tools:layout_editor_absoluteY="232dp" />

        </TableRow>


    </TableLayout>

</android.support.constraint.ConstraintLayout>